# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Application Settings
APP_NAME=Interview Portal
APP_VERSION=1.0.0
DEBUG=True
ENVIRONMENT=development

# Security
SECRET_KEY=your-secret-key-min-32-characters-long-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/interview_portal

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=

# CORS
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Email Configuration (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key
FROM_EMAIL=noreply@interviewportal.com
FROM_NAME=Interview Portal

# Code Execution
PISTON_API_URL=https://emkc.org/api/v2/piston

# WebRTC Configuration
TURN_SERVER_URL=turn:your-turn-server.com:3478
TURN_SERVER_USERNAME=username
TURN_SERVER_CREDENTIAL=credential
STUN_SERVER_URL=stun:stun.l.google.com:19302

# Storage (Supabase Storage)
STORAGE_BUCKET_RESUMES=resumes
STORAGE_BUCKET_RECORDINGS=recordings
STORAGE_BUCKET_AVATARS=avatars

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Monitoring
SENTRY_DSN=

# Logging
LOG_LEVEL=INFO
